{% extends "base.html.twig" %}

{% block title %}
	{{ category.title }}
{% endblock %}

{% block main %}
	<main>

		<article>
			<h2>{{ category.title }}</h2>
			<p>
				{{ category.description }}</p>

			<p>
				{{ category.createdAt | date ('d/m/Y') }}
			</p>
		</article>
	</main>
{% endblock main %}
